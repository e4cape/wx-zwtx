/* 企业账号基本资料 */
.enterprise{height: auto; margin: 0rpx auto;}
.enterprise view{ height:80rpx; line-height: 80rpx; margin: 40rpx 45rpx;}
.enterprise view text{float: left; padding-right: 40rpx; color: #666666; font-size: 26rpx;}
.enterprise view input{float: left; height:80rpx; line-height: 80rpx;color: #999999; font-size: 26rpx; border: 2rpx solid #D7D7D7; padding-left: 20rpx; width: 53.33%;}
/* 企业账号信息 */
.card{ height: 240rpx; margin: 0rpx 0rpx 40rpx; clear: both; overflow: hidden;}
.card .card-title{ height:120rpx; line-height: 120rpx; color: #666666; font-size: 26rpx;}
.card .card-main .card-img{float: left; width: 24.33%; text-align: center;}
.card .card-main .card-img .input{opacity: 0;width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:190rpx;}
.card .card-main .card-img .inputs{opacity: 0;width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:370rpx;}
.card .card-main .card-img .image{ width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:190rpx; }
.card .card-main .card-img .images{ width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:370rpx; }
.card .card-main .card-img .upload{ width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:190rpx;}
.card .card-main .card-img .uploads{ width: 180rpx; height: 180rpx; position: absolute; top: 156rpx; left:370rpx;}
.card .card-main .card-img view{ font-size: 26rpx; color: #666666;  position: relative; top: 200rpx;}
/*采购列表*/
.company-list{margin: 20rpx 30rpx; color: #666666; font-size: 26rpx;}
.company-list view{margin: 20rpx 0rpx;}
.company-list view text{padding-right: 10%;}
/*色条*/
.index-border{height: 20rpx; margin: 20rpx auto; clear: both; background-color: #EDEDED;}
/*申请须知*/
.apply-info{  height: auto;  margin: 0rpx 30rpx; line-height: 40rpx; padding-bottom: 200rpx;}
.apply-info .top{color: #666666; font-size: 28rpx; margin-bottom: 20rpx; text-align: center;}
.apply-info .bottom{color: #666666; font-size: 26rpx;}
/*提交审核*/
.submit{width: 100%; height: 100rpx; line-height: 100rpx; text-align: center;background-color: #DD2B2B; position: fixed;bottom: 0;}
.btn{width: 100%; height: 100rpx; line-height: 100rpx; text-align: center;background-color: #999999; position: fixed;bottom: 0;}
.submit text{ color: #ffffff; font-size: 26rpx;}
.btn text{ color: #ffffff; font-size: 26rpx;}