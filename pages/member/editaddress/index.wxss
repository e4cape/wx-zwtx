/* 编辑地址资料 */
.release{ width: 100%; height: auto; margin: 0rpx auto;}
.release picker{ height:120rpx; line-height: 120rpx; border-bottom: 2rpx solid #D7D7D7; margin: 0rpx 30rpx;}
.release view{ height:120rpx; line-height: 120rpx; border-bottom: 2rpx solid #D7D7D7; margin: 0rpx 30rpx;}
.release picker text{float: left; padding-right: 40rpx; color: #666666; font-size: 26rpx;}
.release view text{float: left; padding-right: 40rpx; color: #666666; font-size: 26rpx;}
.release picker input{float: left; width: 72%; height:120rpx; line-height: 120rpx;color: #999999; font-size: 26rpx;}
.release picker image{float: right; width: 40rpx; height: 40rpx; padding-top:40rpx;}
.release view input{float: left; height:120rpx; line-height: 120rpx;color: #999999; font-size: 26rpx;}
/*提交审核*/
.submit{width: 100%; height: 100rpx; line-height: 100rpx; text-align: center;background-color: #DD2B2B; position: fixed;bottom: 0;}
.submit text{ color: #ffffff; font-size: 26rpx;}